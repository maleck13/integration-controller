            "properties": {
              "text": {
                "kind": "parameter",
                "displayName": "Email text",
                "group": "producer",
                "label": "producer",
                "required": false,
                "type": "textarea",
                "javaType": "java.lang.String",
                "deprecated": false,
                "secret": false,
                "labelHint": "The email message that you want to send.",
                "order": "3"
              },
              "to": {
