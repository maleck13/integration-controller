      "pattern": "To",
      "actionType": "connector",
      "descriptor": {
        "componentScheme": "google-mail",
        "inputDataShape": {
          "kind": "java",
          "type": "io.syndesis.connector.gmail.GmailMessageModel"
        },
        "outputDataShape": {
